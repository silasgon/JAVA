package arrays;

import java.lang.reflect.Array;
import java.util.Arrays;

public class Exercicio {
	public static void main(String[] args) {
		
		double[] notasAlunoA = new double[3];
		notasAlunoA[0] = 7.9;
		notasAlunoA[1] = 8;
		notasAlunoA[2] = 6.7;
		
		Arrays.toString(notasAlunoA);
		System.out.println(notasAlunoA);
		 
	}

}
